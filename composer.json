{
    "name": "root/www",
    "require": {
        "league/plates": "3.4",
        "symfony/var-dumper": "6.0",
        "phpmailer/phpmailer": "^6.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\":"app/"
        },
        "files": [
            "app/helpers/csrf.php",
            "app/helpers/json.php",
            "app/helpers/flash.php",
            "app/helpers/redirect.php",
            "app/helpers/env.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
